begin
a := "Hello World";
b := 1;
c := b * 10;
d := ((a + b) * c) / 10 - b;
if d = 10
then
    while b < 15
    loop
        call proc(a, b, c, d)
    end loop
else
    do
        for (u := (a + b) / c; u > 3; u := u - 1)
        do
            call otherproc(a, b)
        end loop
    until a /= "Hello World"
end if
end


